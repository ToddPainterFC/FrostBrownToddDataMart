                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">With Client (Client_uno,Bills_outstanding) as  (Select  m.client_uno,( Count(Distinct(i.bill_num))) as All_Bills_outstanding from blt_billm a
inner join BLT_bill i on a.BILL_TRAN_UNO=i.TRAN_UNO
inner join HBM_Matter m on a.matter_uno =m.matter_uno
inner join hbm_client c on m.client_uno=c.client_uno
Where ar_status='o'


Group by m.Client_uno),

 Lastpay  (Client_uno,Last_rcpt_date) as (Select tt.client_uno,Max(Last_rcpt_date) from TBM_wip_ar_summary sr
inner join HBM_matter tt on sr.matter_uno=tt.matter_uno
Group by tt.client_uno)

 ,lastbillpay (bill_num,tran_date) as (SELECT distinct(bl.bill_num),MAX(convert(date,am.tran_date))
FROM [SUT_BILL_TRANSACTION] am
inner join BLT_bill bl on am.bill_tran_uno=bl.tran_uno
WHERE  am.TRAN_TYPE in ('CR','RA') and AR_status='o'
and am.ar_total&lt;0
group by bl.Bill_num
)





select p.empl_uno,p.employee_code
	--,p.employee_name
	,c.client_code
	,m.matter_code
	,Concat(c.client_name,' (',c.client_code,')') 'Client' 
	,Concat(m.Matter_name,' (',m.Matter_code,')')'Matter'
	,i.bill_num 'Bill Number',convert(date,i.bill_date)'Bill Date'
	,sum(datediff(day,convert(date,bill_date),getdate()))'Bill Age'
	,sum(a.TOTAL_AR)' Outstanding AR'
	,sum(Client.Bills_outstanding)'Bills Outstanding'
	,sum(total_billed)'Total Billed'
	,Sum(total_paid)'Total Paid'
	,Avg(s.Average_payment_days) 'AVG Pay Days'
	,dateadd(day,Avg(s.Average_payment_days),Convert(date,Bill_date)) 'Expected Payment Date'
	,Datediff(day,dateadd(day,Avg(s.Average_payment_days),Convert(date,Bill_date)),Getdate()) 'Days Late'
	,su.last_rcpt_date as 'Matter Last Payment Date'
	,lastpay.last_rcpt_date 'Last Client Payment Date'
	,(lastbillpay.tran_date) as 'last Bill Payment Date' 
	, (Case When ((dateadd(day,Avg(s.Average_payment_days),Convert(date,Bill_date+5))&lt;Convert(Date,Getdate())
and sum(datediff(day,convert(date,bill_date),getdate()))&lt;='365') and a.total_ar&gt;='5000') Then 'R' End) 'Priority'
from SUM_BILL a
inner join HBM_Matter m on a.matter_uno =m.matter_uno
inner join TBM_Matter t on m.matter_uno =t.matter_uno
inner join TBM_billgrp b on t.billgrp_uno=b.BILLGRP_UNO
inner join CLM_payor_summary s on b.client_uno =s.payr_client_uno
inner join HBM_persnl p on m.bill_empl_uno =p.empl_uno
inner join HBM_client c on m.client_uno=c.client_uno
inner join BLT_bill i on a.BILL_TRAN_UNO=i.TRAN_UNO
inner join Client on c.client_uno=Client.Client_uno
inner join TBM_wip_AR_summary su on a.matter_uno =su.matter_uno
inner join Lastpay on c.client_uno=lastpay.client_uno
left outer join lastbillpay on i.bill_num=lastbillpay.bill_num
Where a.TOTAL_AR &gt;0

--and s.average_Payment_days&lt;&gt;'0'
group by p.empl_uno,p.employee_code,p.employee_name,c.client_code,C.client_name,m.matter_code,m.matter_name,i.bill_num,i.bill_date,su.last_rcpt_date, lastpay.last_rcpt_date,lastbillpay.tran_date,a.TOTAL_AR
--,lastbillpay.tran_date
--having dateadd(day,Avg(s.Average_payment_days),Convert(date,Bill_date+5))&lt;Convert(Date,Getdate())
--and sum(datediff(day,convert(date,bill_date),getdate()))&lt;='365'
--Order by Priority desc, [ Outstanding AR] desc</property>
