                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">declare @startdate as date = (select dateadd(month,-12,Me_date) from tbm_parms);
declare @enddate as date = (select me_date from tbm_parms)


select tt.TK_EMPL_UNO, avg(datediff(day, tt.TRAN_DATE,tt.POST_DATE)) avgDays, (Case When avg(datediff(day, tt.TRAN_DATE,tt.POST_DATE)) &gt;='5' Then 'R' When avg(datediff(day, tt.TRAN_DATE,tt.POST_DATE)) between '2' and '4' Then 'Y' Else 'G' End) Color
from tat_time tt
inner join HBM_PERSNL pp on pp.EMPL_UNO = tt.TK_EMPL_UNO
where tt.TRAN_DATE between @startdate  and  @enddate -- get previous 12 months transactions 
and POST_DATE&gt;= TRAN_DATE -- exclude future dated transactions
group by tt.TK_EMPL_UNO</property>
