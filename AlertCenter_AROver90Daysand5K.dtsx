                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">with cte_bills as (
select amt.BILL_EMPL_UNO, bb.billgrp_uno, amt.BILL_tran_UNO
from BLT_BILL_AMT amt
join blt_bill bb on bb.tran_uno=amt.bill_tran_uno
where amt.TRAN_TYPE = 'bl'
and amt.tran_date between dateadd(day,-365, getdate()) and dateadd(day, -90, getdate())),

cte_unpaid as(
select bb.*, sum(amt.sign * (amt.fees_amt+ amt.hard_amt + amt.soft_amt)) CHARGES 
from cte_bills bb
join blt_bill_amt amt on amt.BILL_TRAN_UNO = bb.BILL_TRAN_UNO
group by bb.BILL_EMPL_UNO , bb.bill_tran_uno, bb.BILLGRP_UNO
having sum(amt.sign *(amt.fees_amt+ amt.hard_amt + amt.soft_amt) ) &gt; 0)

SELECT pp.employee_code, count(*) unpid , (Case When count(*)&gt;='4' Then 'R' When count(*) between '2' and '3' then 'Y' Else 'G' End) color
FROM cte_unpaid uu
join HBM_PERSNL pp on pp.empl_uno = uu.bill_empl_uno
where not exists(
SELECT  BAL_ZERO_NO_FCA
FROM BLT_BILL_AMT AMT
WHERE AMT.BILL_TRAN_UNO = uu.BILL_TRAN_UNO
AND AMT.BAL_ZERO_NO_FCA = 'y')
and uu.charges &gt;= 5000
group by pp.employee_name, employee_code</property>
