                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">With Exceptions as (Select tkinit,dayofwk,(Hoursexception) 'hours' from 
(Select tkinit,Monday,Tuesday ,Wednesday,Thursday,Friday from [ADR_FBT].dbo.DT_Exceptions Where active='1') as p
unpivot(hoursexception For dayofwk in (Monday ,Tuesday ,Wednesday,Thursday,Friday)) as unpvt
Group by tkinit,dayofwk,hoursexception
),


Missing as ((Select p.empl_uno,Tran_date,sum(base_hrs)'Hours worked',(isnull((Case When (convert(date,t.tran_date))=convert(date,yr.holiday_date) then 0 else (exceptions.hours) end),6))'Hours scheduled', (Case when sum(base_hrs)&lt;(isnull((Case When (convert(date,t.tran_date))=convert(date,yr.holiday_date) then 0 else (exceptions.hours) end),6)) then 1 else 0 end) 'Missingdays'  from TAT_Time t
inner join HBM_persnl p on t.tk_empl_uno=p.empl_uno
left outer join Exceptions on p.employee_code=Exceptions.tkinit and (Datename(dw,Tran_date))=Exceptions.dayofwk
left outer join [HBA_HOLIDAY_YR] yr on t.tran_date=yr.HOLIDAY_DATE
INNER JOIN [ADR_FBT].[dbo].[MRT_Title] mt on p.POSITION = mt.POSITION
WHERE TERMINATE_DATE is null AND Timekeeper='Include' AND EMPLOYEE_NAME not LIKE 'Tbd%'
AND p.PROF&lt;&gt;'108'
AND p.OFFC &lt;&gt; '27'
and ENTRY_STATUS in ('U','R')
and datename(dw,t.tran_date) in ('Monday' ,'Tuesday' ,'Wednesday','Thursday','Friday')
and t.TRAN_DATE &gt;=Dateadd(month,-3,GetDate())
and Tran_date&lt;Convert(date,getdate())
--and p.employee_code='11757'
Group by p.empl_uno,Tran_date,yr.Holiday_date,Exceptions.hours)
)

Select empl_uno, Sum(missingdays) 'days', (Case When Sum(missingdays)&gt;='4'Then'R' When Sum(missingdays) between '2' and '3' Then 'Y' Else 'G' End) color from missing
Group by empl_uno</property>
